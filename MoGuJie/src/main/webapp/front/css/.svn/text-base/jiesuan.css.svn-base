@charset "utf-8";
/* CSS Document */
#content{
	width:100%;
	height:110px;
	border-top:1px solid #DDDDDD;
	border-bottom:1px solid #DDDDDD;
	}
#content #content1{
	float:left;
	width:225px;
	height:48px;
	background-color: transparent;
	background:url(../image/gouwuche2.png) no-repeat -450px 0px;
	margin-top:20px;
	margin-left:100px;
	}
#content #content2{
	float:right;
	margin-top:20px;
	margin-right:40px;
	}
#text{
	margin-top:40px;
	width:1200px;
	background:#FFF;}
#text #text1{
	width:100%;
	height:250px;
	}
.text11{
	font-size:16px;
	margin-left:30px;
	padding-top:20px;
	}
.text12{
	margin-left:30px;
	margin-top:10px;
	float:left;
	width:240px;
	height:190px;
	background:url(../image/gouwuche.png) no-repeat 0px 0px;}
.text12 p{
	font-size:14px;
	line-height:35px;
	padding-left:20px;}
#text #text2{
	width:100%;
	}
.text21{
	font-size:16px;
	margin-left:30px;
	}
.text22 table{
	border-collapse:collapse;
	margin-top:5px;
	width:1150px;
	}
.text221{
	border-top:1px solid #EFEFEF;
	border-bottom:1px solid #EFEFEF;
	height:50px;
	}
.text22 tr td{
	text-align:center;
	font-size:14px;
	color:#333333;}
.text222{
	width:100%;
	height:40px;
	}
.text22 .text222 td{
	text-align:left;
	color:#333333;}
.text22 .text223{
	width:100%;
	height:130px;
	background:#F8F8F8;
	}
.hanban{
	display:block;
	float:left;
	margin-left:30px;
	text-decoration:none;
	color:#333333;
	}
.img1{
	float:left;
	padding-left:10px;
	width:100px;
	height:100px;
	}




#foot{
	border-top:1px solid #EFEFEF;
	background:#FFF;
	width:1200px;
	height:70px;}
.foot1{
	width:600px;
	height:100%;
	float:left;}
.foot11{
	padding-top:20px;
	float:left;
	padding-left:30px;
	width:40px;
	height:100%}

.foot13{
	display:block;
	float:right;
	padding-top:20px;
	padding-right:450px;
	text-decoration:none;
	color:#666666;
	}
.foot13:hover{
	text-decoration:underline;}
.foot2{
	width:200px;
	height:100%;
	float:left;
	padding-top:20px;}
.foot2 label{
	color:#FF63A9;}

.foot3{
	width:200px;
	height:100%;
	float:left;
	}
.foot31{
	float:left;
	display:block;
	width:50px;
	padding-top:20px;}
.foot32{
	display:block;
	float:right;
	font-size:28px;
	color:#FF63A9;
	padding-top:15px;
	padding-right:30px;
	}
.foot4{
	width:200px;
	height:100%;
	background:#FFA701;
	float:left;
	text-align:center;}
.foot4 a{
	display:block;
	font-size:28px;
	color:#fff;
	padding-top:15px;
	text-decoration:none;}
#end{
	margin-top:60px;
	margin-left:400px;
	width:100%;
	height:50px;
	background:url(../image/gouwuche2.png) no-repeat 0px -42px;}
.haha{
	font-size: 12px;
	color:#666;
	line-height:30px;
	margin-left:300px;
	margin-top: 120px;
}
.haha a{
	text-decoration: none;
	padding-top: 20px;
}