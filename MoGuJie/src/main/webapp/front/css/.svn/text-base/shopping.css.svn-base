@charset "utf-8";
/* CSS Document */
*{
		margin:0px auto;
		padding:0px;	
	}
body{
	width:1200px;		
	margin:0px auto;
	height:auto;
	margin-top:50px;
	background:url(../images/bg.png);
}
#title{
	margin-left:-80px;
}
#title #title-right{
	float:right;
	list-style:none;
	width:42px;
	height:auto;
	background:#000;
	margin-right:-80px;
	margin-top:-130px;
}
#title #black{
	list-style:none;
	width:20px;
}
#title #black li{
	font-size:15px;
	color:#FFF;
	margin-top:30px;
}
#title1{
	width:1200px;
	height:70px;

}
#title1 ul{
	float:left;
	list-style:none;
}
#title1 ul li{
	border-right:1px solid #999;
	float:left;
	height:10px;
	line-height:10px;
	margin-top:30px;
	margin-left:30px;
	width:100px;
}
#title1 ul .tit1{
	width:100px;
	height:40px;
	background:#333;
	margin-top:20px;
	color:#FFF;
	line-height:30px;
}
#title1 ul .tit1 a{
	list-style:none;
	float:left;
	padding-top:5px;
	padding-left:5px;
	cursor:pointer;
}
#title1 ul .tit2{
	margin-left:-60px;
}
#title1 ul .foot{
	border-right:0;
}
#button{
	width:1200px;
	height:80px;
	background:#FFF;
}
#button ul{
	float:left;
	list-style:none;
	width:1140px;
	height:80px;
	margin-left:30px;
	border-bottom:1px solid #CCC;
}
#button ul li{
	float:left;
	margin-left:20px;
	font-size:14px;
	margin-top:30px;
	line-height:25px;
}
#button ul .bt{
	margin-left:20px;
}
#button ul .bt input{
	margin-right:5px;
}
#button ul .bt span{
	list-style:none;
	float:right;
	margin-left:5px;
}
#button ul .goods{
	margin-left:120px;
}
#button ul .goodsinfo{
	margin-left:200px;
}
#button ul .price{
	margin-left:100px;
}
#button ul .num{
	margin-left:140px;
}
#button ul .sum{
	margin-left:130px;
}
#button ul .manager{
	margin-left:80px;
}
#content{
	background:#FFF;
	width:1200px;
	height:auto;
}
#content #shopinfo .shop{
	float:left;
	height:60px;
}
#content #shopinfo .shop td{
	float:left;
	line-height:30px;
	list-style:none;
}
#content #shopinfo .shop .shop1{
	margin-left:50px;
	margin-top:15px;
}
#content #shopinfo .shop .shop1 span{
	color:#666;
	list-style:none;
	float:right;
	margin-left:5px;
}
#content #shopinfo .shop .shop2{
	margin-left:10px;
	margin-top:15px;
	font-size:14px;
}
#content #shopinfo .shop .shop2 a:hover{
	text-decoration:underline;
	cursor:pointer;
}
#content #shopinfo .shop .shop3{
	margin-left:10px;
	margin-top:20px;
}

#content #shopinfo .info{
	list-style:none;
	float:left;
	width:1140px;
	height:150px;
	margin-left:30px;
	border-bottom:1px solid #CCC;
	border-top:1px solid #CCC;
}
#content #shopinfo .info td{
	float:left;
	line-height:30px;
	list-style:none;
}
#content #shopinfo .info .btu{
	margin-left:20px;
	margin-top:70px;
	width:20px;
}
#content #shopinfo .info .pic{
	margin-left:20px;
	margin-top:30px;
	cursor:pointer;
	width:100px;
}
#content #shopinfo .info .pic1{
	margin-top:-10px;
	margin-left:150px;
	position:absolute;
}
#content #shopinfo .info .passage{
	margin-left:30px;
	margin-top:20px;
	font-size:14px;
	line-height:30px;
	color:#666;
	width:160px;
}
#content #shopinfo .info .passage a:hover{
	text-decoration:underline;
	color:#F9C;
	cursor:pointer;
}
#content #shopinfo .info .xinfo{
	margin-left:70px;
	margin-top:20px;
	font-size:14px;
	color:#333;
	width:70px;
}
#content #shopinfo .info .price{
	margin-left:90px;
	margin-top:20px;
	width:80px;
	
}
#content #shopinfo .info .price .yprice{
	text-decoration:line-through;
	color:#999;	
}
#content #shopinfo .info .price .xprice{
	color:#000;
	font-weight:bold;
}
#content #shopinfo .info .price span{
	color:#F60;
}
#content #shopinfo .info .number{
	margin-left:90px;
	margin-top:50px;
	width:100px;
}

#content #shopinfo .info .number .nums{
	border:1px #333333 solid;
	list-style:none;
	height:25px;
	text-align:center;
	margin-left:20px;
}
#content #shopinfo .info .sum{
	margin-left:90px;
	margin-top:50px;
	font-weight:bold;
	width:40px;
	color:#F36;
}

#content #shopinfo .info .delete{
	margin-left:90px;
	margin-top:50px;
}
#content #shopinfo .info .delete a:hover{
	text-decoration:underline;
	cursor:pointer;
}
#select{
	background:#FFF;
	width:1200px;
	height:60px;
	border-top:1px solid #CCC;
}
#select ul{
	list-style:none;
	float:left;
}
#select ul li{
	float:left;
	line-height:30px;
}

#select ul .sel2{
	margin-left:40px;
	margin-top:20px;
	text-decoration:underline;
	color:#666;
}
#select ul .sel2 a:hover{
	cursor:pointer;
}
#select ul .sel3{
	margin-left:680px;
	margin-top:15px;
	color:#666;
}
#select ul .sel3 .sele{
	font-size:18px;
	margin-left:5px;
	margin-right:5px;
	color:#F00;
}
#select ul .sel4{
	margin-left:5px;
	margin-top:15px;
	font-size:18px;
	font-weight:bold;
	color:#F00;
	width:56px;
}

#select ul .sel5{
	margin-left:20px;
	width:150px;
	height:60px;
	background:#CCC;
}
#select ul .sel5 a{
	list-style:none;
	float:left;
	font-size:24px;
	color:#FFF;
	padding-top:15px;
	padding-left:25px;
	cursor:pointer;
	height:45px;
	width:125px;
}
.pic img{
	width:100px;
	height:100px;
}
.pic1 img{
	padding-left:5px;
	padding-top:5px;
	padding-right:10px;
	width:240px;
	height:240px;
}
.a{
	display: inline-block;
	background:url("../images/beijin.png") no-repeat scroll;
	width:26ppx;
	height:260px;
}
